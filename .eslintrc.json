{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": "off"
    // "implicit-arrow-linebreak": ["error", "below", "beside"]
  }
}
